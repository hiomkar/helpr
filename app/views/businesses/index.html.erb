<h1>Listing businesses</h1>

<table>
  <tr>
    <th>Admin</th>
    <th>Biz name</th>
    <th>Biz url</th>
    <th>Existing biz url</th>
    <th>Start hour</th>
    <th>End hour</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @businesses.each do |business| %>
  <tr>
    <td><%= business.admin_id %></td>
    <td><%= business.biz_name %></td>
    <td><%= business.biz_url %></td>
    <td><%= business.existing_biz_url %></td>
    <td><%= business.start_hour %></td>
    <td><%= business.end_hour %></td>
    <td><%= link_to 'Show', business %></td>
    <td><%= link_to 'Edit', edit_business_path(business) %></td>
    <td><%= link_to 'Destroy', business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Business', new_business_path %>
